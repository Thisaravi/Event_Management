<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
		http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
		version="3.1">
		
	<servlet>
	
		<servlet-name>login</servlet-name>
		<servlet-class>EventPlanner.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/log</url-pattern>
	</servlet-mapping>
	
	<servlet>
	
		<servlet-name>pending</servlet-name>
		<servlet-class>EventPlanner.PendingEventsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pending</servlet-name>
		<url-pattern>/PendingS</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Insert</servlet-name>
		<servlet-class>EventPlanner.eventInsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Insert</servlet-name>
		<url-pattern>/insert</url-pattern>
		
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Planned</servlet-name>
		<servlet-class>EventPlanner.PlannedServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Planned</servlet-name>
		<url-pattern>/planned</url-pattern>
		
	</servlet-mapping>
	
	<servlet>
		<servlet-name>delete</servlet-name>
		<servlet-class>EventPlanner.DeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/Delete</url-pattern>
		
	</servlet-mapping>
	
	<servlet>
		<servlet-name>updateServlet</servlet-name>
		<servlet-class>EventPlanner.updateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateServlet</servlet-name>
		<url-pattern>/updateEvent</url-pattern>
		
	</servlet-mapping>
	
</web-app>